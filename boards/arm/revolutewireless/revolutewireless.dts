

/dts-v1/;
#include <nordic/nrf52833_qiaa.dtsi>


/ {
	model = "revolutewireless";
	compatible = "revolutewireless";

	chosen {

		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
		zephyr,uart-mcumgr = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,code-partition = &slot0_partition;
		// zephyr,settings-partition = &storage_partition;

	};


	
	leds {
			compatible = "gpio-leds";
			blueled: led_0 {
					/* LED 0 on P0.03, LED on when pin is high */
					gpios = < &gpio0 3 GPIO_ACTIVE_HIGH >;
					label = "LED 0";
			};
			nc_1: led_1 {
					/* LED 1 on P0.14, LED on when pin is low */
					gpios = < &gpio0 10 GPIO_ACTIVE_HIGH >;
					label = "LED 1";
			};
			nc_2: led_2 {
					/* LED 2 on P1.0, on when low */
					gpios = < &gpio0 9 GPIO_ACTIVE_HIGH >;
					label = "LED 2";
			};
			nc_3: led_3 {
					/* LED 3 on P1.1, on when high */
					gpios = < &gpio0 5 GPIO_ACTIVE_HIGH >;
					label = "LED 3";
			};
	};




 


	buttons {
			compatible = "gpio-keys";
			/*
				* Add up to 4 total buttons in child nodes as shown here.
				*/
			button_right: button_0 {
					/* Button 0 on P0.11. Enable internal SoC pull-up
						* resistor and treat low level as pressed button. */
					gpios = <&gpio0 28 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
					label = "Button 0";
			};
			button_left: button_1 {
					/* Button 1 on P0.12. Enable internal pull-down resistor.
						* Treat high level as pressed button. */
					gpios = <&gpio0 29 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
					label = "Button 1";
			};
			button_up: button_2 {
					/* Button 2 on P1.12, enable internal pull-up,
						* low level is pressed. */
					gpios = <&gpio0 30 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
					label = "Button 2";
			};
			button_down: button_3 {
					/* Button 3 on P1.15, no internal pull resistor,
						* low is pressed. */
					gpios = <&gpio0 31 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
					label = "Button 3";
			};
	};


	

	/* These aliases are provided for compatibility with samples */
	aliases {

		led0 = &blueled;
		sw0 = &button_right;
		sw1 = &button_left;
		sw2 = &button_up;
		sw3 = &button_down;

		// led0 = &led0_red;
		// led0-red   = &led0_red;	
		// sw0 =  &button1;


	};
};

&gpiote {
	status = "okay";
};

&gpio0 {
	status = "okay";
};

&gpio1 {
	status = "okay";
};


&flash0 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		boot_partition: partition@0 {
			label = "mcuboot";
			reg = <0x00000000 0xC000>;
		};
		slot0_partition: partition@c000 {
			label = "image-0";
			reg = <0x0000C000 0x37000>;
		};
		slot1_partition: partition@43000 {
			label = "image-1";
			reg = <0x00043000 0x37000>;
		};
		storage_partition: partition@7a000 {
			label = "storage";
			reg = <0x0007A000 0x00006000>;
		};
	};
};




&i2c0 {

	status = "okay";
	pinctrl-0 = <&i2c0_default>;
	pinctrl-names = "default";
	
	// as5600: as5600@36 {
	// 	compatible = "ams,as5600";
		
	// 	reg = <0x36>;
	// 	status = "okay";
	// };

	// bq27421@55 {
	// 	compatible = "ti,bq274xx";
	// 	reg = <0x55>;
	// 	design-voltage = <3700>;
	// 	design-capacity = <40>;
	// 	taper-current = <45>;
	// 	terminate-voltage = <3000>;
	// };

};


&pinctrl {
	i2c0_default: i2c0_default {
		group1 {
			psels = <NRF_PSEL(TWIM_SCL, 0,15)>, <NRF_PSEL(TWIM_SDA, 0, 17)>;
		};
	};
};


